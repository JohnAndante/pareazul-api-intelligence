{
  "name": "pareazul-assistant-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "check": "bun run type-check && bun run lint"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@langchain/openai": "^0.6.11",
    "@supabase/supabase-js": "^2.57.4",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "langchain": "^0.3.33",
    "multer": "^2.0.2",
    "redis": "^5.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  }
}
