# Environment template - copy to .env and fill with your values

# Server Configuration
PORT=3000
NODE_ENV=development

# Pareazul API
PAREAZUL_API_WEBSERVICE=https://ws-dev.pareazul.com.br
PAREAZUL_API_BACKEND=https://api-backend.dev.pareazul.com.br

# Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# OpenAI Configuration
OPENAI_API_KEY=sk-your_openai_api_key_here

# Security
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
API_BEARER_TOKEN=mock-secret-key-for-testing

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads
TEMP_DIR=temp

# Audio Processing
AUDIO_MAX_DURATION=300

# Session Management
SESSION_TTL=3600
MEMORY_BUFFER_SIZE=20
VECTOR_DIMENSIONS=1536

# External Services Health Check URLs
OPENAI_HEALTH_URL=https://status.openai.com
